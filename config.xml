<?xml version='1.0' encoding='utf-8'?>
<widget id="com.concretedata.cordovaPluginFileTestHarness" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova Plugin File Tests</name>
    <description>
        A harness for running the cordova-plugin-file-tests test suite.
    </description>
    <author email="tim.hambourger@concretedata.com">
        Tim Hambourger
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="cdvfile:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-file" spec="file:../cordova-plugin-file" />
    <plugin name="cordova-plugin-file-tests" spec="file:../cordova-plugin-file/tests" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
